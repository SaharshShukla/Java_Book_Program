
public class TwoD_Array_Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int [][] flats= {{101,102,103},{201,202,203}};

for(int i=0;i<flats.length;i++){
	for(int j=0;j<flats[i].length;j++){
System.out.println("flats[0][0]= "+flats[0][0]+" loop "+flats[0][0]);
System.out.println("flats[0][1]= "+flats[0][1]+" loop "+i+j);
System.out.println("flats[0][2]= "+flats[0][2]+" loop "+i+j);
System.out.println("flats[1][0]= "+flats[1][0]+" loop "+i+j);
System.out.println("flats[1][1]= "+flats[1][1]+" loop "+i+j);
System.out.println("flats[1][2]= "+flats[1][2]+" loop "+i+j);
}
	}


	}

}
